    --bg-gis-second-primary         : {{ settings.bg_gis_second_primary | default: '#f1e8da' }};
    --bg-second                     : {{ settings.bg_second | default: '#fffdf1' }};
    --font-monospace                : monospace;
    --font-primary                  : {{ settings.font_primary.family | default: 'F37Blanka-Medium' }};
    --font-primary-light            : {{ settings.font_primary_light.family | default: 'F37Blanka-Light' }};
    --font-sohne                    : {{ settings.font_sohne.family | default: '"SohneMono", monospace' }} {{ settings.font_sohne.fallback_families }};
    --gig-black                     : {{ settings.gig_black | default: '#000' }};
